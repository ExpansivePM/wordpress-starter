{
	"parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
	"extends": ["eslint:recommended", "standard", "prettier"],
	"env": {
		"browser": true,
    "jest": true
	},
	"plugins": ["prettier"],
    "rules": {
        "no-unused-vars": 1,
        "no-new": 0,
        "space-before-function-paren": 0,
        "prettier/prettier": "error"
    },
	"globals": {
		"jquery": "readonly",
		"$": "readonly",
		"wp": "readonly",
        "oneDS": "readonly",
        "oneds_object": "readonly"
	}
}
